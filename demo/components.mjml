<mjml>
  <mj-head>
    <ola-head></ola-head>
    <mj-title>{{ name }}, you can now access our SEO tool</mj-title>
    <mj-preview>Let's get more potential clients to your site!</mj-preview>
  </mj-head>

  <mj-body>
    <ola-header
      logo="https://marketgoo.github.io/Ola-Emails/img/logo-brand.png"
    />

    <ola-panel>
      <!-- Primera fila - 3 columnas iguales -->
      <mj-section padding="0 20px">
        <!-- Primera columna - Score (con componente score) -->
        <mj-column width="33.33%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/speedometer-gray.png"
              text="Score"
            ></ola-card-header>

            <ola-card-content>
              <ola-score size="100" value="80"></ola-score>
            </ola-card-content>
          </ola-card>
        </mj-column>

        <!-- Segunda columna - Site Audit (sin componente score) -->
        <mj-column width="33.33%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/storefront-gray.png"
              text="Site Audit"
            ></ola-card-header>

            <ola-card-content>
              <ola-text
                variant="font-4-medium"
                color="color-warning-500"
                align="center"
                >3</ola-text
              >
              <ola-text
                variant="font-0-regular"
                color="color-neutral-700"
                align="center"
                >Pending tasks</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>

        <!-- Tercera columna - Content (sin componente score) -->
        <mj-column width="33.33%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/clipboard-text-gray.png"
              text="Content"
            ></ola-card-header>

            <ola-card-content>
              <mj-section padding="0">
                <mj-column>
                  <ola-text
                    variant="font-4-medium"
                    color="color-negative-500"
                    align="center"
                    >19</ola-text
                  >
                </mj-column>
                <mj-column>
                  <ola-text variant="font-0-regular" color="color-neutral-700"
                    >/28</ola-text
                  >
                </mj-column>
              </mj-section>

              <ola-text
                variant="font-0-regular"
                color="color-neutral-700"
                align="center"
                >Pending vs Optimized Pages</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>
      </mj-section>

      <!-- Segunda fila - 1 columna ancha (66%) y 1 columna normal (33%) -->
      <mj-section padding="0 20px">
        <!-- Primera columna ancha - Traffic -->
        <mj-column width="66.66%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/graph-gray.png"
              text="Traffic"
            ></ola-card-header>

            <ola-card-content>
              <ola-text variant="font-1-regular"
                >Información de Traffic</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>

        <!-- Segunda columna - Traffic -->
        <mj-column width="33.33%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/graph-gray.png"
              text="Traffic"
            ></ola-card-header>

            <ola-card-content>
              <ola-text variant="font-4-medium" align="center">34 s</ola-text>
              <ola-text
                variant="font-0-regular"
                color="color-neutral-700"
                align="center"
                >Average visit duration</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>
      </mj-section>

      <!-- Tercera fila - 2 columnas iguales (50% cada una) -->
      <mj-section padding="0 20px">
        <!-- Primera columna - Backlinks -->
        <mj-column width="50%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/lego-gray.png"
              text="Backlinks"
            ></ola-card-header>

            <ola-card-content>
              <ola-text variant="font-1-regular"
                >Información de Backlinks</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>

        <!-- Segunda columna - Extra -->
        <mj-column width="50%" padding="10px">
          <ola-card>
            <ola-card-header
              src="https://apps.marketgoo.com/themes/mktgoo/img/icons/speedometer-gray.png"
              text="Otro elemento"
            ></ola-card-header>

            <ola-card-content>
              <ola-text variant="font-1-regular"
                >Información adicional</ola-text
              >
            </ola-card-content>
          </ola-card>
        </mj-column>
      </mj-section>
    </ola-panel>

    <mj-include path="./partials/footer-default.mjml" />
  </mj-body>
</mjml>
