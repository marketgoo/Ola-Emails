{
  "name": "@marketgoo/ola-emails",
  "description": "Ola design system implemented for emails, using MJML",
  "version": "0.1.0",
  "scripts": {
    "start": "nodemon --exec gulp demo",
    "build": "gulp build",
    "publish:demo": "gulp demo && gh-pages -d demo"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0"
  },
  "dependencies": {
    "mjml": "^4.5.1",
    "nodemon": "^1.19.4"
  },
  "nodemonConfig": {
    "watch": [
      "demo/",
      "components/"
    ],
    "ext": "js,mjml"
  }
}
