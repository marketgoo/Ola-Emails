{
  "name": "@marketgoo/ola-emails",
  "description": "Ola design system implemented for emails, using MJML",
  "version": "0.1.0",
  "main": "src",
  "scripts": {
    "start": "nodemon --exec gulp demo",
    "publish:demo": "rm -f demo/*.html && gulp demo && gh-pages -d demo",
    "prettier": "prettier --single-quote 'src/*.js' --tab-width 4 --print-width=120 --write"
  },
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@marketgoo/ola-themes": "https://marketgoo.gitlab.io/ola-themes/nightly.tgz",
    "mjml": "^4.5.1"
  },
  "nodemonConfig": {
    "watch": [
      "demo/",
      "src/",
      "tokens.js"
    ],
    "ext": "js,mjml"
  }
}
